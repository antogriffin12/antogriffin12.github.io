{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Employment rate - UK",
      "anchor": "start",
      "subtitle": "Ages 16 and over, % | Source: ONS through ECO API",
      "fontSize": 14,
      "subtitleFontSize": 12,
      "color": "#000000",
      "subtitleColor": "#000000"
    },
    "height": 300,
    "width": 500,
    "background": "#ffffff",
    "view": {
      "fill": "#ffffff",
      "strokeOpacity": 0
    },
    "data": {
      "url": "https://api.economicsobservatory.com/gbr/empl?vega",
      "format": {
        "type": "json"
      }
    },
    "mark": {
      "type": "line",
      "color": "#0066cc",
      "strokeWidth": 2
    },
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      }
    }
}